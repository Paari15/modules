{
    "name": "Anime Nexus",
    "version": "1.0.0",
    "author": "Paari15",
    "description": "Fetches anime details and streams from Anime Nexus.",
    "id": "anime-nexus",
    "main": "animeNexus.js",
    "methods": {
        "search": {
            "name": "searchResults",
            "type": "search",
            "required": ["query"]
        },
        "details": {
            "name": "extractDetails",
            "type": "details",
            "required": ["id"]
        },
        "stream": {
            "name": "extractStreamUrl",
            "type": "stream",
            "required": ["id"]
        }
    }
}
